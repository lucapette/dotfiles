{
  "$schema": "https://opencode.ai/config.json",
  "theme": "nord",
  "model": "deepseek/deepseek-chat",
  "provider": {
    "qwen-coding-plan": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Qwen Coding Plan",
      "options": {
        "baseURL": "https://coding-intl.dashscope.aliyuncs.com/v1",
        "apiKey": "{env:QWEN_API_KEY}"
      },
      "models": {
        "qwen3-coder-plus": {
          "name": "qwen3-coder-plus"
        }
      }
    }
  },
  "agent": {
    "build": {
      "mode": "primary",
      "model": "deepseek/deepseek-reasoner",
      "temperature": 0.0,
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    },
    "plan": {
      "mode": "primary",
      "model": "deepseek/deepseek-reasoner",
      "temperature": 0.0,
      "tools": {
        "write": false,
        "edit": false,
        "bash": false
      }
    },
    "fixer": {
      "mode": "primary",
      "model": "qwen-coding-plan/qwen3-coder-plus",
      "temperature": 0.0,
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    }
  },
  "mcp": {
    "chrome-devtools": {
      "type": "local",
      "command": ["npx", "-y", "chrome-devtools-mcp@latest"],
      "enabled": false
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}"
      },
      "enabled": false
    }
  }
}
