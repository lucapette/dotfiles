{
  "$schema": "https://opencode.ai/config.json",
  "model": "qwen/qwen3-coder-plus",
  "provider": {
    "qwen": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Qwen Coding Plan",
      "options": {
        "baseURL": "https://coding-intl.dashscope.aliyuncs.com/v1",
        "apiKey": "{env:QWEN_API_KEY}"
      },
      "models": {
        "qwen3-coder-plus": {
          "name": "qwen3-coder-plus"
        },
        "qwen3-max-2026-01-23": {
          "name": "qwen3-max-2026-01-23"
        }
      }
    }
  },
  "agent": {
    "build": {
      "mode": "primary",
      "model": "qwen/qwen3-coder-plus",
      "temperature": 0.0,
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    },
    "plan": {
      "mode": "primary",
      "model": "deepseek/deepseek-reasoner",
      "temperature": 0.0,
      "tools": {
        "write": false,
        "edit": false,
        "bash": false
      }
    },
    "creative-planner": {
      "mode": "primary",
      "model": "deepseek/deepseek-chat",
      "temperature": 1.5,
      "tools": {
        "write": false,
        "edit": false,
        "bash": false
      }
    },
    "deep-coder": {
      "mode": "primary",
      "model": "deepseek/deepseek-reasoner",
      "temperature": 0.0,
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    },
    "qwen-coder-max": {
      "mode": "primary",
      "model": "qwen/qwen3-max-2026-01-23",
      "temperature": 0.0,
      "tools": {
        "write": true,
        "edit": true,
        "bash": true
      }
    }
  },
  "mcp": {
    "chrome-devtools": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "enabled": false
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}"
      },
      "enabled": false
    }
  }
}
